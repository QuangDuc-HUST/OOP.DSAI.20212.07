<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.RowConstraints?>
<?import javafx.scene.layout.StackPane?>

<StackPane alignment="CENTER_LEFT" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="dsai.group07.force.controller.StatisticsPanelController">
   <children>
      <Label fx:id="posLabel" text="Label" textFill="ORANGE">
         <StackPane.margin>
            <Insets left="25.0" />
         </StackPane.margin>
      </Label>
      <Label fx:id="accLabel" text="Label" textFill="ORANGE">
         <StackPane.margin>
            <Insets left="25.0" top="40.0" />
         </StackPane.margin></Label>
      <Label fx:id="velLabel" text="Label" textFill="ORANGE" StackPane.alignment="CENTER_LEFT">
         <StackPane.margin>
            <Insets left="25.0" top="80.0" />
         </StackPane.margin>
      </Label>
      <Label fx:id="aForceLabel" text="Label" textFill="ORANGE">
         <StackPane.margin>
            <Insets left="25.0" top="120.0" />
         </StackPane.margin></Label>
      <Label fx:id="fForceLabel" text="Label" textFill="ORANGE">
         <StackPane.margin>
            <Insets left="25.0" top="160.0" />
         </StackPane.margin></Label>
      <Label fx:id="sumForceLabel" text="Label" textFill="ORANGE">
         <StackPane.margin>
            <Insets left="25.0" top="200.0" />
         </StackPane.margin></Label>
      <GridPane>
        <columnConstraints>
          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
        </columnConstraints>
        <rowConstraints>
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        </rowConstraints>
      </GridPane>
   </children>
</StackPane>
