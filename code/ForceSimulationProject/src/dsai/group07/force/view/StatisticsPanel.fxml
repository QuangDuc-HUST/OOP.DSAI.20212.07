<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.CheckBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.StackPane?>

<StackPane alignment="CENTER_LEFT" prefHeight="150.0" prefWidth="200.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="dsai.group07.force.controller.StatisticsPanelController">
   <children>
      <Label fx:id="angLabel" text="Label" textFill="ORANGE">
         <StackPane.margin>
            <Insets left="100.0" top="-200.0" />
         </StackPane.margin>
      </Label>
      <Label fx:id="angAccLabel" text="Label" textFill="ORANGE">
         <opaqueInsets>
            <Insets left="25.0" top="-80.0" />
         </opaqueInsets>
         <StackPane.margin>
            <Insets left="100.0" top="-150.0" />
         </StackPane.margin>
      </Label>
      <Label fx:id="angVelLabel" text="Label" textFill="ORANGE">
         <opaqueInsets>
            <Insets />
         </opaqueInsets>
         <StackPane.margin>
            <Insets left="100.0" top="-100.0" />
         </StackPane.margin>
      </Label>
      <Label fx:id="posLabel" text="Label" textFill="ORANGE">
         <StackPane.margin>
            <Insets left="100.0" top="-50.0" />
         </StackPane.margin>
      </Label>
      <Label fx:id="accLabel" text="Label" textFill="ORANGE">
         <StackPane.margin>
            <Insets left="100.0" />
         </StackPane.margin></Label>
      <Label fx:id="velLabel" text="Label" textFill="ORANGE" StackPane.alignment="CENTER_LEFT">
         <StackPane.margin>
            <Insets left="100.0" top="50.0" />
         </StackPane.margin>
      </Label>
      <Label fx:id="aForceLabel" text="Label" textFill="ORANGE">
         <StackPane.margin>
            <Insets left="100.0" top="100.0" />
         </StackPane.margin></Label>
      <Label fx:id="fForceLabel" text="Label" textFill="ORANGE">
         <StackPane.margin>
            <Insets left="100.0" top="150.0" />
         </StackPane.margin></Label>
      <Label fx:id="sumForceLabel" text="Label" textFill="ORANGE">
         <StackPane.margin>
            <Insets left="100.0" top="200.0" />
         </StackPane.margin></Label>
      <Label fx:id="massLabel" layoutX="110.0" layoutY="235.0" text="0 kg" textFill="ORANGE">
         <StackPane.margin>
            <Insets left="100.0" top="250.0" />
         </StackPane.margin>
      </Label>
      <CheckBox fx:id="angCheckBox" mnemonicParsing="false" text="Angular">
         <StackPane.margin>
            <Insets top="-200.0" />
         </StackPane.margin>
      </CheckBox>
      <CheckBox fx:id="angAccCheckBox" layoutX="10.0" layoutY="50.0" mnemonicParsing="false" text="Ang Acc">
         <StackPane.margin>
            <Insets top="-150.0" />
         </StackPane.margin>
      </CheckBox>
      <CheckBox fx:id="angVelCheckBox" layoutX="10.0" layoutY="110.0" mnemonicParsing="false" text="Ang Vel">
         <StackPane.margin>
            <Insets top="-100.0" />
         </StackPane.margin>
      </CheckBox>
      <CheckBox fx:id="posCheckBox" layoutX="10.0" layoutY="110.0" mnemonicParsing="false" text="Position">
         <StackPane.margin>
            <Insets top="-50.0" />
         </StackPane.margin>
      </CheckBox>
      <CheckBox fx:id="accCheckBox" layoutX="10.0" layoutY="110.0" mnemonicParsing="false" text="Accelerate">
         <StackPane.margin>
            <Insets />
         </StackPane.margin>
      </CheckBox>
      <CheckBox fx:id="velCheckBox" layoutX="10.0" layoutY="110.0" mnemonicParsing="false" text="Velocity">
         <StackPane.margin>
            <Insets top="50.0" />
         </StackPane.margin>
      </CheckBox>
      <CheckBox fx:id="aForceCheckBox" layoutX="10.0" layoutY="110.0" mnemonicParsing="false" text="AForce">
         <StackPane.margin>
            <Insets top="100.0" />
         </StackPane.margin>
      </CheckBox>
      <CheckBox fx:id="fForceCheckBox" layoutX="10.0" layoutY="110.0" mnemonicParsing="false" text="FForce">
         <StackPane.margin>
            <Insets top="150.0" />
         </StackPane.margin>
      </CheckBox>
      <CheckBox fx:id="sumForceCheckBox" layoutX="10.0" layoutY="110.0" mnemonicParsing="false" text="NetForce">
         <StackPane.margin>
            <Insets top="200.0" />
         </StackPane.margin>
      </CheckBox>
      <CheckBox fx:id="massCheckBox" layoutX="10.0" layoutY="210.0" mnemonicParsing="false" text="Mass">
         <StackPane.margin>
            <Insets top="250.0" />
         </StackPane.margin>
      </CheckBox>
      <CheckBox fx:id="appliedForceCheckBox" mnemonicParsing="false" onAction="#appliedForceView" text="appliedForce">
         <StackPane.margin>
            <Insets top="350.0" />
         </StackPane.margin>
      </CheckBox>
      <CheckBox fx:id="netForceCheckBox" mnemonicParsing="false" onAction="#netForceView" text="netForce">
         <StackPane.margin>
            <Insets top="300.0" />
         </StackPane.margin>
      </CheckBox>
      <CheckBox fx:id="frictionForceCheckBox" layoutX="10.0" layoutY="360.0" mnemonicParsing="false" onAction="#frictionForceView" text="frictionForce">
         <StackPane.margin>
            <Insets top="400.0" />
         </StackPane.margin>
      </CheckBox>
      <ImageView fx:id="rightAppliedForce" fitHeight="32.0" fitWidth="200.0" pickOnBounds="true" visible="false">
         <image>
            <Image url="@../../../../../resources/images/vector.png" />
         </image>
      </ImageView>
      <ImageView fx:id="rightFrictionForce" fitHeight="32.0" fitWidth="200.0" pickOnBounds="true" visible="false">
         <image>
            <Image url="@../../../../../resources/images/vector.png" />
         </image>
      </ImageView>
      <ImageView fx:id="rightNetForce" fitHeight="32.0" fitWidth="200.0" pickOnBounds="true" visible="false">
         <image>
            <Image url="@../../../../../resources/images/vector.png" />
         </image>
      </ImageView>
      <ImageView fx:id="leftAppliedForce" fitHeight="32.0" fitWidth="200.0" pickOnBounds="true" rotate="180.0" visible="false">
         <image>
            <Image url="@../../../../../resources/images/vector.png" />
         </image>
      </ImageView>
      <ImageView fx:id="leftNetForce" fitHeight="32.0" fitWidth="200.0" pickOnBounds="true" rotate="180.0" visible="false">
         <image>
            <Image url="@../../../../../resources/images/vector.png" />
         </image>
      </ImageView>
      <ImageView fx:id="leftFrictionForce" fitHeight="32.0" fitWidth="200.0" pickOnBounds="true" rotate="180.0" visible="false">
         <image>
            <Image url="@../../../../../resources/images/vector.png" />
         </image>
      </ImageView>
   </children>
</StackPane>
